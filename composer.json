{
    "name": "maxiewright/orms",
    "type": "project",
    "description": "The management app of Military Orderly Rooms.",
    "keywords": [
        "HRMS"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bezhansalleh/filament-shield": "^3.0@beta",
        "bfinlay/laravel-excel-seeder": "^3.3",
        "filament/filament": "^3.2",
        "flowframe/laravel-trend": "^0.2",
        "jeffgreco13/filament-breezy": "*",
        "larabug/larabug": "^3.1",
        "laravel/framework": "^11.34",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "leandrocfe/filament-apex-charts": "^3.0",
        "masbug/flysystem-google-drive-ext": "^2.2",
        "pxlrbt/filament-excel": "^2.0",
        "ralphjsmit/laravel-filament-onboard": "^2.3",
        "spatie/laravel-backup": "^8.6",
        "spatie/laravel-sluggable": "^3.6",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^2.8",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-laravel": "^2.2",
        "pestphp/pest-plugin-livewire": "^2.1",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-stubs": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan spatie-stub:publish --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "format": [
            "./vendor/bin/pint"
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "ralphjsmit/packages": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.ralphjsmit.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
